{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/api/*",
      "backend": "firefishportfolioapi",
      "rewrite": "/api/*"
    }
  ],
  "backend": {
    "backendName": "firefishportfolioapi",
    "backendBaseUrl": "https://firefishportfolioapi.azurewebsites.net" 
  },
  "globalHeaders": {
    "Cache-Control": "no-store, max-age=0"
  },
  "environmentVariables": {
    "REACT_APP_REDIRECT_URI": "",
    "REACT_APP_API_BASE_URL": "https://firefishportfolioapi.azurewebsites.net"
  }
} 